{
  "name": "clearsky-api",
  "version": "1.0.0",
  "type": "module",
  "description": "ClearSky API client for React Native and web applications",
  "main": "dist/api.js",
  "types": "dist/api.d.ts",
  "exports": {
    ".": {
      "types": "./dist/api.d.ts",
      "import": "./dist/api.js"
