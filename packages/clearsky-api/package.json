    "@types/jest": "^29.5.12",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~9.2.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.3.4",
    "typescript": "~5.8.3",
    "msw": "^2.4.9"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-native": ">=0.70.0"
  }
