{
  "name": "@akari/devtools-current-route",
  "version": "0.1.0",
  "private": true,
  "description": "DevTools plugin that displays the active Expo Router pathname.",
  "main": "src/index.ts",
  "types": "src/index.ts",
  "sideEffects": false,
  "scripts": {
    "build": "npx expo-module build",
    "build:all": "npx expo-module prepare && npm run web:export",
    "clean": "npx expo-module clean",
    "prepare": "npx expo-module prepare && npm run web:export",
    "web:dev": "cd webui && npx expo start -w",
    "web:export": "node ./scripts/build-webui.js"
  },
  "files": [
    "src",
    "dist",
    "expo-module.config.json"
  ],
  "keywords": [
    "expo",
    "devtools"
  ],
  "peerDependencies": {
    "expo": "*",
    "react": "*",
    "react-native": "*"
  },
  "devDependencies": {
    "@types/react": "~19.1.10",
    "expo": "^54.0.10",
    "expo-module-scripts": "^4.1.7",
    "react": "19.1.0",
    "react-native": "0.81.4",
    "typescript": "~5.9.2"
  }
}
