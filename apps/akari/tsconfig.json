{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "paths": {
      "@/*": ["./*"],
      "@/bluesky-api": ["../../packages/bluesky-api"],
      "@/clearsky-api": ["../../packages/clearsky-api"],
      "@/libretranslate-api": ["../../packages/libretranslate-api"],
      "@/teal-api": ["../../packages/teal-api"],
      "@/tenor-api": ["../../packages/tenor-api"]
    }
  },
  "include": ["**/*.ts", "**/*.tsx", "**/*.json", ".expo/types/**/*.ts", "expo-env.d.ts"]
}
